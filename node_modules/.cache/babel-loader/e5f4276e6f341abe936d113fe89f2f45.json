{"ast":null,"code":"import DeleteIcon from \"./icons/DeleteIcon.vue\";\nimport BaseButton from \"./UI/Buttons/BaseButton.vue\";\nimport BaseIconButton from \"./UI/Buttons/BaseIconButton.vue\";\nimport IconBase from \"./UI/BaseIcon.vue\";\nimport { useRouter } from \"vue-router\";\nimport { useLikeStore } from \"@/store/useLikeStore\";\nimport { storeToRefs } from \"pinia\";\nexport default {\n  __name: 'Likes',\n  emits: [\"onClick\"],\n\n  setup(__props, {\n    expose,\n    emit\n  }) {\n    expose();\n    const likeStore = useLikeStore();\n    const {\n      likes\n    } = storeToRefs(likeStore);\n    const router = useRouter();\n\n    const unLike = product => {\n      likeStore.handleLikes(product);\n    };\n\n    const goToProductPage = id => {\n      router.push(`/product/${id}`);\n      emit(\"onClick\", \"likes\");\n    };\n\n    const __returned__ = {\n      emit,\n      likeStore,\n      likes,\n      router,\n      unLike,\n      goToProductPage,\n      DeleteIcon,\n      BaseButton,\n      BaseIconButton,\n      IconBase,\n      useRouter,\n      useLikeStore,\n      storeToRefs\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n\n};","map":{"version":3,"mappings":"AA4CA,OAAOA,UAAP,MAAuB,wBAAvB;AACA,OAAOC,UAAP,MAAuB,6BAAvB;AACA,OAAOC,cAAP,MAA2B,iCAA3B;AACA,OAAOC,QAAP,MAAqB,mBAArB;AACA,SAASC,SAAT,QAA0B,YAA1B;AACA,SAASC,YAAT,QAA6B,sBAA7B;AACA,SAASC,WAAT,QAA4B,OAA5B;;;;;;;;;;IAIA,MAAMC,SAAS,GAAGF,YAAY,EAA9B;IACA,MAAM;MAAEG;IAAF,IAAYF,WAAW,CAACC,SAAD,CAA7B;IAEA,MAAME,MAAM,GAAGL,SAAS,EAAxB;;IAEA,MAAMM,MAAM,GAAIC,OAAD,IAAa;MAC1BJ,SAAS,CAACK,WAAV,CAAsBD,OAAtB;IACD,CAFD;;IAIA,MAAME,eAAe,GAAIC,EAAD,IAAQ;MAC9BL,MAAM,CAACM,IAAP,CAAa,YAAWD,EAAG,EAA3B;MACAE,IAAI,CAAC,SAAD,EAAY,OAAZ,CAAJ;IACD,CAHD","names":["DeleteIcon","BaseButton","BaseIconButton","IconBase","useRouter","useLikeStore","storeToRefs","likeStore","likes","router","unLike","product","handleLikes","goToProductPage","id","push","emit"],"sources":["/Users/tamirlankarimov/Desktop/Vue_Project_ecommerce/e-commerce-app/src/components/Likes.vue"],"sourcesContent":["<template>\n  <ul class=\"likes\">\n    <li\n      v-for=\"likedProduct in likes\"\n      :key=\"likedProduct.id\"\n      class=\"likes__item\"\n    >\n      <img\n        :src=\"likedProduct.image\"\n        :alt=\"likedProduct.title\"\n        class=\"likes__image\"\n        @click=\"goToProductPage(likedProduct.id)\"\n      />\n      <div class=\"likes__product-info\">\n        <span\n          class=\"likes__product-info-title\"\n          @click=\"goToProductPage(likedProduct.id)\"\n          >{{ likedProduct.title }}</span\n        >\n\n        <div class=\"likes__product-info-container\">\n          <div class=\"likes__rating\">\n            <img src=\"../assets/star-icon.svg\" />\n            <p class=\"card__rating-text\">{{ likedProduct.rating.rate }}</p>\n          </div>\n          <span class=\"likes__price\"\n            ><strong>${{ likedProduct.price }}</strong></span\n          >\n        </div>\n      </div>\n      <BaseIconButton\n        @click=\"unLike(likedProduct)\"\n        variant=\"contained\"\n        iconHoverColor=\"#ef2525\"\n        iconColor=\"#74747474\"\n        opacity=\"1\"\n      >\n        <DeleteIcon />\n      </BaseIconButton>\n    </li>\n  </ul>\n</template>\n\n<script setup>\nimport DeleteIcon from \"./icons/DeleteIcon.vue\";\nimport BaseButton from \"./UI/Buttons/BaseButton.vue\";\nimport BaseIconButton from \"./UI/Buttons/BaseIconButton.vue\";\nimport IconBase from \"./UI/BaseIcon.vue\";\nimport { useRouter } from \"vue-router\";\nimport { useLikeStore } from \"@/store/useLikeStore\";\nimport { storeToRefs } from \"pinia\";\n\nconst emit = defineEmits([\"onClick\"]);\n\nconst likeStore = useLikeStore();\nconst { likes } = storeToRefs(likeStore);\n\nconst router = useRouter();\n\nconst unLike = (product) => {\n  likeStore.handleLikes(product);\n};\n\nconst goToProductPage = (id) => {\n  router.push(`/product/${id}`);\n  emit(\"onClick\", \"likes\");\n};\n</script>\n\n<style>\n.likes {\n  display: flex;\n  flex-direction: column;\n  list-style: none;\n  padding: 0;\n}\n\n.likes__item {\n  display: grid;\n  align-items: center;\n  grid-template-columns: 40px 4fr 50px;\n  gap: 20px;\n  padding: 20px 10px;\n  border-bottom: 1px solid lightgray;\n}\n\n.likes__item:last-of-type {\n  border-bottom: none;\n}\n\n.likes__product-info {\n  display: flex;\n  flex-direction: column;\n  align-items: flex-start;\n  gap: 5px;\n}\n\n.likes__product-info-title {\n  text-align: left;\n  cursor: pointer;\n}\n\n.likes__product-info-title:hover {\n  transition: color 0.1s linear;\n  color: #ed8939;\n}\n\n.likes__product-info-container {\n  display: flex;\n  gap: 10px;\n  color: #747474;\n  font-size: 12px;\n}\n\n.likes__image {\n  cursor: pointer;\n  width: 40px;\n  height: 40px;\n  object-fit: contain;\n}\n\n.likes__image:hover {\n  opacity: 0.7;\n  transition: opacity 0.2s linear;\n}\n\n.likes__rating {\n  display: flex;\n  gap: 5px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}